{
  "name": "patternplate-server",
  "version": "0.8.2",
  "description": "Programmatically serve patternplate components via a well defined REST API",
  "main": "library/",
  "scripts": {
    "start": "node --harmony binary/patternplate-server.js",
    "console": "boilerplate-console",
    "build": "npm run babel",
    "watch": "npm run babel -- --watch",
    "babel": "npm run clean && babel source --out-dir ./",
    "clean": "rm -rf application binary configuration library"
  },
  "bin": {
    "patternplate-server": "binary/patternplate-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.sinnerschrader.com:patternplate/patternplate-server.git"
  },
  "keywords": [
    "duo",
    "REST",
    "JSON"
  ],
  "author": {
    "name": "Team UX",
    "email": "team-ux@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Mario Nebl",
      "email": "mario.nebl@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "iojs": ">= 1.4.3",
    "node": ">= 0.12.0",
    "npm": ">= 2.5.1"
  },
  "dependencies": {
    "archiver": "^0.14.4",
    "babel-core": "^5.2.17",
    "babelify": "^6.0.2",
    "bluebird": "^2.9.25",
    "boilerplate-server": "git+ssh://git@git.sinnerschrader.com:patternplate/boilerplate-server.git#v0.6.1",
    "browserify": "^10.2.4",
    "core-util-is": "^1.0.1",
    "envify": "^3.4.0",
    "git-rev": "^0.2.1",
    "isomorphic-fetch": "^2.0.2",
    "jsx-transform": "^2.0.0",
    "koa-send": "^1.3.1",
    "less": "^2.5.0",
    "less-plugin-autoprefix": "^1.4.1",
    "less-plugin-clean-css": "^1.5.0",
    "less-plugin-npm-import": "^2.0.0",
    "less-plugin-pattern-import": "git+ssh://git@git.sinnerschrader.com:patternplate/less-plugin-pattern-import.git#v0.1.1",
    "lodash": "^3.9.3",
    "lodash.merge": "^3.3.1",
    "lru-cache": "^2.6.3",
    "marked": "^0.3.3",
    "memory-streams": "^0.1.0",
    "minimatch": "^2.0.8",
    "minimist": "^1.1.1",
    "normalize.less": "^1.0.0",
