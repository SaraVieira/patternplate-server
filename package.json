{
  "name": "patternplate-server",
  "version": "0.4.8",
  "description": "Programmatically serve patternplate components via a well defined REST API",
  "main": "library/",
  "scripts": {
    "start": "node --harmony binary/patternplate-server.js",
    "build": "npm run babel",
    "watch": "npm run babel -- --watch",
    "babel": "npm run clean && babel source --out-dir ./",
    "clean": "rm -rf application binary configuration library"
  },
  "bin": {
    "patternplate-server": "binary/patternplate-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.sinnerschrader.com:patternplate/patternplate-server.git"
  },
  "keywords": [
    "duo",
    "REST",
    "JSON"
  ],
  "author": {
    "name": "Team UX",
    "email": "team-ux@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Mario Nebl",
      "email": "mario.nebl@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "iojs": ">= 1.4.3",
    "node": ">= 0.12.0",
    "npm": ">= 2.5.1"
  },
  "dependencies": {
    "babel-core": "^5.2.17",
    "babelify": "^6.0.2",
    "bluebird": "^2.9.25",
    "boilerplate-server": "git+ssh://git@git.sinnerschrader.com:patternplate/boilerplate-server.git#v0.5.2",
    "browserify": "^10.1.0",
    "core-util-is": "^1.0.1",
    "isomorphic-fetch": "^2.0.2",
    "jsx-transform": "^2.0.0",
    "less": "^2.5.0",
    "less-plugin-autoprefix": "^1.4.1",
    "less-plugin-clean-css": "^1.5.0",
    "less-plugin-npm-import": "^2.0.0",
    "less-plugin-pattern-import": "git+ssh://git@git.sinnerschrader.com:patternplate/less-plugin-pattern-import.git",
    "lru-cache": "^2.6.3",
    "marked": "^0.3.3",
    "minimist": "^1.1.1",
    "normalize.less": "^1.0.0",
    "pascal-case": "^1.1.0",
    "pretty-data": "^0.40.0",
    "q-io": "^1.12.0",
    "rc": "^1.0.1",
    "react": "^0.13.2",
    "react-jsx": "^0.13.0",
    "require-all": "^1.0.0",
    "uglifyify": "^3.0.1"
  },
  "devDependencies": {
    "babel": "^5.2.17",
    "babel-eslint": "^3.0.1",
    "eslint": "^0.20.0"
  }
}
