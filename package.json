    "require-all": "^1.0.0",
    "semver": "^4.3.6",
    "through": "^2.3.7",
    "uglify-js": "^2.4.23",
    "uglifyify": "^3.0.1",
    "vinyl": "^0.5.0"
  },
  "devDependencies": {
    "babel": "^5.2.17",
    "babel-eslint": "^3.0.1",
    "eslint": "^0.20.0"
  }
}
