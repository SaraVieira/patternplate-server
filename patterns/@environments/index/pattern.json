{
	"name": "index",
	"version": "0.1.0",
	"conditional-comment": "!IE",
	"formats": ["js", "css", "less"],
	"environment": {
		"transforms": {
			"less": {
				"opts": {
					"globalVars": {
						"global-is-legacy": true
					}
				}
			},
			"react-jsx": {
				"opts": {
					"context": {
						"env": "index"
					}
				}
			}
		}
	}
}
